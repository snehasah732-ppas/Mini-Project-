#include <stdio.h>
#include <stdlib.h>
#include <time.h>
int main()
{
    char str[5][100], str2[5][100];
    int available_slots[5];
    int time1, time2;
    int j, temp;
    srand((unsigned)time(NULL));
    for (int i = 0; i < 5; i++) {
        available_slots[i] = i + 1;
    }
    for (int i = 4; i > 0; i--) {
        j = rand() % (i + 1);

        temp = available_slots[i];
        available_slots[i] = available_slots[j];
        available_slots[j] = temp;
    }
    for(int i = 0; i < 5; i++)
    {
        printf("Enter the subject: ");
        scanf("%s", str[i]) ;
        printf("Enter the teacher's name: ");
        scanf("%99s", str2[i]);
        printf("\n");
    }
    for(int i = 0; i < 5; i++)
    {
        time1 = available_slots[i];
        time2 = time1 + 1;
s
        printf("%s - %s : %d:00 to %d:00\n", str[i], str2[i], time1, time2);
    }
    return 0;
}
