#include <stdio.h>

int main() {
    FILE *fp;
    char ch;
    char filename[50];

    printf("Enter file name: ");
    scanf("%s", filename);

    fp = fopen(filename, "r");

    if (fp == NULL) {
        printf("Error! Cannot open file.\n");
        return 0;
    }

    printf("\nContents of the file:\n\n");
    while ((ch = fgetc(fp)) != EOF) {
        putchar(ch);
    }

    fclose(fp);
    return 0;
}
